{
  "name": "clubz",
  "version": "1.0.0",
  "description": "ClubZ",
  "main": "index.js",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/*.js'",
    "prebuild": "npm run build:clean",
    "build": "npm run build:clean && cross-env NODE_ENV=production BROWSERSLIST_ENV=production webpack --config config/webpack.production.config.js --color --profile --progress",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production node server",
    "start:production": "npm run build && npm run start:prod",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-lint": "eslint --ext .tsx --ext .ts src/",
    "lint-fix": "eslint . ./src/**/*.tsx --fix",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test-all": "npm run check-lint && npm run format && npm run build",
    "browser-list": "cross-env BROWSERSLIST_ENV=development npx browserslist",
    "browser-list:prod": "cross-env BROWSERSLIST_ENV=production npx browserslist",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
